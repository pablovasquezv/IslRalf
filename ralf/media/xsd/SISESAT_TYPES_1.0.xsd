                <xsd:annotation>
                    <xsd:documentation>Falla en sistema de navegación de la embarcación (vehículos acuáticos)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5499">
                <xsd:annotation>
                    <xsd:documentation>Otras fallas del vehículo Especificar:</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5501">
                <xsd:annotation>
                    <xsd:documentation>Diseño riesgoso de máquinas y/o herramientas o utilización de ellas para fines no concebidos.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5502">
                <xsd:annotation>
                    <xsd:documentation>Modificaciones realizadas en la máquina que dan lugar a situaciones de riesgo no previstas por el fabricante.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5503">
                <xsd:annotation>
                    <xsd:documentation>Acceso inadecuado a la máquina o deficiente ubicación</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5504">
                <xsd:annotation>
                    <xsd:documentation>Máquinas sin enclavamiento o estibación segura para la realización de la tarea</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5505">
                <xsd:annotation>
                    <xsd:documentation>Falta o falla de elementos de protección y aislación de máquinas</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5506">
                <xsd:annotation>
                    <xsd:documentation>Materiales defectuosos o de mala calidad en máquinas que involucran altos cambios de energía (eléctrica, calor, flujo de masa, energía química, hidráulica)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5507">
                <xsd:annotation>
                    <xsd:documentation>Fallas en el sistema eléctrico, neumático o hidráulico</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5508">
                <xsd:annotation>
                    <xsd:documentation>Ausencia de dispositivos de control de emanaciones de las máquinas</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5509">
                <xsd:annotation>
                    <xsd:documentation>Fallas en el sistema de control y comando de la máquina</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5510">
                <xsd:annotation>
                    <xsd:documentation>Visibilidad insuficiente Sistema de iluminación de máquina inexistente, inadecuado, en mal estado o deficiente</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5511">
                <xsd:annotation>
                    <xsd:documentation>Inexistencia de manual de operación o instrucciones en la máquina. O ilegibles para el operador.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5512">
                <xsd:annotation>
                    <xsd:documentation>Parte estructurales de máquinas, vehículos, equipos y/o herramientas en mal estado</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5513">
                <xsd:annotation>
                    <xsd:documentation>Ausencia o deficiencias de sistemas de bloqueo automático de funcionamiento</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5514">
                <xsd:annotation>
                    <xsd:documentation>Sistemas de puesta en marcha o paro de la máquina fuera de diseño o sin sistema de parada de emergencia</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5515">
                <xsd:annotation>
                    <xsd:documentation>Máquinas sin dispositivos anti explosión en atmósferas explosivas</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5516">
                <xsd:annotation>
                    <xsd:documentation>Ausencia de alarmas de puesta en marcha y funcionamiento</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5599">
                <xsd:annotation>
                    <xsd:documentation>Otras causas relativas a máquinas y herramientas Especificar</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="6101">
                <xsd:annotation>
                    <xsd:documentation>Desastres naturales: Inundaciones terremotos, maremotos, erupciones volcánicas imprevistas, aluviones, incendios forestales.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="6102">
                <xsd:annotation>
                    <xsd:documentation>Desastres tecnológicos: Explosiones, incendio, fugas radiactivas reactor, colapsos de grandes estructuras</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="6103">
                <xsd:annotation>
                    <xsd:documentation>Acciones delictuales de personas: Delitos de terceros, actos de sabotaje, actos vandálicos, atentados, suicidios</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="6104">
                <xsd:annotation>
                    <xsd:documentation>Accidentes involuntarios provocados por terceros no relacionados con la empresa de la víctima</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="7999">
                <xsd:annotation>
                    <xsd:documentation>Otros factores no considerados en ninguno de los grupos principales anteriores Especificar:</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="STTipoNotificacion">
        <xsd:restriction base="xsd:positiveInteger">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Notificacion sin multa</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Notificacion con multa</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <!-- STMulta_RALF_historico incluye todos los valores historicos de STMulta_RALF. Se usa para efectos de visualizacion -->
    <xsd:simpleType name="STMulta_RALF_historico">
        <xsd:restriction base="xsd:positiveInteger">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Aplicación de Art. 80</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Recargo por D.S. 67 art. 5</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation>Recargo por D.S. 67 art. 1</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation>Ambos recargos (art. 5 y art. 15 D.S. 67)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="STMulta_RALF">
        <xsd:restriction base="xsd:positiveInteger">
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Recargo por D.S. 67 art. 5</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation>Recargo por D.S. 67 art. 1</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation>Ambos recargos (art. 5 y art. 15 D.S. 67)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="STMulta">
        <xsd:restriction base="xsd:positiveInteger">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Aplicación de Art. 80</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Recargo por D.S. 67 art. 5</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation>Recargo por D.S. 67 art. 1</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="CTAplicacionMulta_RALF">
        <xsd:sequence>
            <xsd:element name="tipo_multa" type="STMulta_RALF"/>
            <xsd:element name="fecha_inicio_multa" type="xsd:date"/>
            <xsd:element name="fecha_fin_multa" type="xsd:date"/>
            <xsd:element name="monto_multa" type="xsd:nonNegativeInteger" minOccurs="0"/>
            <xsd:element name="recargo" type="xsd:decimal" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CTAplicacionMulta">
        <xsd:sequence>
            <xsd:element name="tipo_multa" type="STMulta" minOccurs="0" />
            <xsd:element name="fecha_inicio_multa" type="xsd:date" minOccurs="0" />
            <xsd:element name="fecha_fin_multa" type="xsd:date" minOccurs="0" />
            <xsd:element name="monto_multa" type="xsd:nonNegativeInteger" minOccurs="0" />
            <xsd:element name="recargo" type="xsd:decimal" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="STOrigen_diagnostico"> 
        <xsd:annotation> 
          <xsd:documentation>Origen del diagnostico</xsd:documentation> 
        </xsd:annotation> 
        <xsd:restriction base="xsd:positiveInteger"> 
            <xsd:enumeration value="1"> 
              <xsd:annotation> 
                <xsd:documentation>Origen laboral</xsd:documentation> 
              </xsd:annotation> 
            </xsd:enumeration> 
            <xsd:enumeration value="2"> 
            <xsd:annotation> 
              <xsd:documentation>Origen no laboral</xsd:documentation> 
            </xsd:annotation> 
            </xsd:enumeration> 
          </xsd:restriction> 
    </xsd:simpleType> 
    <xsd:complexType name="CTCodificacion_Vehiculo_involucrado">
        <xsd:sequence>
            <xsd:element name="codigo_modo_transporte" type="STCodigo_modo_transporte"/>
            <xsd:element name="codigo_papel_lesionado" type="STCodigo_papel_lesionado"/>
            <xsd:element name="codigo_contraparte" type="STCodigo_contraparte"/>
            <xsd:element name="codigo_tipo_evento" type="STCodigo_Tipo_evento"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CTGeolocalizacion">
        <xsd:sequence>
            <xsd:element name="geo_latitud" type="STCoordenadaLatitud"/>
            <xsd:element name="geo_longitud" type="STCoordenadaLongitud"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="STCoordenadaLatitud">
        <xsd:annotation>
            <xsd:documentation>COORDENADA 2 ENTEROS 7 DECIMALES</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="(-?[0-9]{1,2}\.[0-9]{7})"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="STCoordenadaLongitud">
        <xsd:annotation>
            <xsd:documentation>COORDENADA 3 ENTEROS 7 DECIMALES</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="(-?[0-9]{1,3}\.[0-9]{7})"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="STNodo">
        <xsd:annotation>
            <xsd:documentation>NODO DE 9 DIGITOS, SEPARADO POR PUNTOS Y EMPIEZAN DESDE EL 1</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="([1-9]{1}(\.[0-9]{1}){8})"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="CTNodo_causa">
        <xsd:sequence>
            <xsd:element name="ubicacion_nodo" type="STNodo"/>
            <xsd:element name="codigo_causa" type="STCodigoCausa"/>
            <xsd:element name="glosa_causa" type="STTexto"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CTPrescripcionMedida_RALF">
        <xsd:sequence>
            <xsd:element name="folio_medida_prescrita" type="xsd:positiveInteger"/>
            <xsd:element name="tipo_medida_prescrita" type="STTipoMedidaPrescrita"/>
            <xsd:element name="codigo_medida" type="STTexto" minOccurs="0"/>
            <xsd:element name="descripcion_medida_prescrita" type="STTexto"/>
            <xsd:element name="medida_inmediata" type="STSiNo"/>
            <xsd:element name="fecha_plazo_cumplimiento_medida" type="xsd:date"/>
            <xsd:element name="codigo_causa" type="STCodigoCausa" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="STTipoMedidaPrescrita">
        <xsd:annotation>
            <xsd:documentation>Clasificacion de medida</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Medida de control Ingenieril</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Medida de control Administrativo</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation>Medida de control Proteccion Personal</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="CT_ZONA_ZVM_RALF">
        <xsd:sequence>
            <xsd:element name="verificacion_medida" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Verificacion Medidas</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="fecha_verificacion" type="xsd:date"/>
                        <xsd:element name="folio_medida_prescrita" type="xsd:positiveInteger"/>
                        <xsd:element name="datos_verificacion" type="CTDatosVerificacion_RALF"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CTDatosVerificacion_RALF">
        <xsd:sequence>
            <xsd:element name="cumplimiento_medida" type="STCumplimientoMedida"/>
            <xsd:element name="observacion_verificacion" type="STTexto" minOccurs="0"/>
            <xsd:element name="fecha_cumple_medida_empleador" type="xsd:date"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="STCumplimientoMedida">
        <xsd:annotation>
            <xsd:documentation>Conjunto de datos que definen una verificacion de medida</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:positiveInteger">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Cumple medida prescrita por el OA</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Cumple implementando medida equivalente o superior, distinta a la prescrita por el OA</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation>No cumple, no implementando o implementando deficientemente medida prescrita por el OA</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation>No cumple, implementando medida deficiente  distinta a la prescrita por el OA</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="STCausaNotificacion">
        <xsd:annotation>
            <xsd:documentation>Causales de notificacion a la Autoridad</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Se identifica peligro o agente de riesgo que requiere notificacion a la Autoridad</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Nivel de riesgo sobre el limite establecido en la norma</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation> cualitativa con incumplimiento verificado en fecha </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation>Incumplimiento de medidas prescritas por el OA</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5">
                <xsd:annotation>
                    <xsd:documentation>Entidad Empleadora, no entrega nómina de trabajadores expuestos en plazo establecido</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="STCausaNotificacion_RALF">
        <xsd:annotation>
            <xsd:documentation>Causales de notificacion a la Autoridad</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Se identifica peligro o agente de riesgo que requiere notificacion a la Autoridad</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Nivel de riesgo sobre el limite establecido en la norma</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation> cualitativa con menos de 30% de exposicion a silice todos los ciuu</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation>Incumplimiento de medidas prescritas por el OA</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5">
                <xsd:annotation>
                    <xsd:documentation>Empleador impide el cumplimiento de Circular</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="CTNotificaAutoridad">
        <xsd:sequence>
            <xsd:element name="fecha_notificacion_autoridad" type="xsd:date"/>
            <xsd:element name="autoridad_receptora" type="STAutoridad"/>
            <xsd:element name="region_autoridad_receptora" type="STCodigoRegion"/>
            <xsd:element name="receptor_autoridad" type="CTReceptorAutoridad"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CTNotificaAutoridad_RALF">
        <xsd:sequence>
            <xsd:element name="fecha_notificacion_autoridad" type="xsd:date"/>
            <xsd:element name="autoridad_receptora">
                <xsd:simpleType>
                    <xsd:restriction base="STAutoridad">
                        <xsd:pattern value="1|2" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="region_autoridad_receptora" type="STCodigoRegion"/>
            <xsd:element name="receptor_autoridad" type="CTReceptorAutoridad"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="STAutoridad">
        <xsd:annotation>
            <xsd:documentation>Autoridad receptora de notificacion</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>DIRECCION DEL TRABAJO</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>SEREMI DE SALUD</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation>SUSESO</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="CTReceptorAutoridad">
        <xsd:annotation>
            <xsd:documentation>datos del profesional de la autoridad notificada</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="rut_profesional_autoridad" type="STRut"/>
            <xsd:element name="apellido_paterno_autoridad" type="STTexto"/>
            <xsd:element name="apellido_materno_autoridad" type="STTexto"/>
            <xsd:element name="nombres_autoridad" type="STTexto"/>
            <xsd:element name="correo_elect_resp_Autoridad" type="STEmail"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="STDocumento_identidad">
        <xsd:sequence>
            <xsd:element name="origen_doc_identidad" type="STOrigen_identidad"/>
            <xsd:element name="identificador" type="STTextoRut"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="STOrigen_identidad">
        <xsd:annotation>
            <xsd:documentation>Origen Documento de Identificacion</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:positiveInteger">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Nacional</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Extranjero</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="STEstadoCentroTrabajo">
        <xsd:annotation>
            <xsd:documentation>Estado de Centro de Trabajo</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Activo</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Caduco</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="STHttpURL">
        <xsd:restriction base="xsd:anyURI">
            <xsd:pattern value="https?://.+" />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="CTAccidente_por_falta_medidas_prevencion">
        <xsd:sequence>
            <xsd:element name="falta_medidas_prevencion" type="STSiNo" />
            <xsd:element name="fundamento_falta_medidas_prevencion" type="CTDescripcion" />
            <xsd:element name="antecedentes_falta_medidas_prevencion" type="STAntecedentes_falta_medidas_prevencion" />
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="STAntecedentes_falta_medidas_prevencion">
        <xsd:annotation>
            <xsd:documentation>Debe consignar si la conclusión se fundamenta exclusivamente en la investigación del organismo administrador, o además, en sanciones aplicadas por los organismos fiscalizadores.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Conclusión se fundamenta exclusivamente en los antecedentes recopilados por el OA durante la investigación</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Conclusión se fundamenta además en las infracciones constatadas por los organismos fiscalizadores (SEREMI de Salud o Dirección del Trabajo)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="CTDescripcion">
        <xsd:sequence>
            <xsd:element name="describir" type="STTexto" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CTResolucion_de_recargo">
        <xsd:annotation>
            <xsd:documentation>Información de  la resolucion del recargo de tasa</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Tipo_resolucion_informada" type="STTipo_resolucion_informada"/>
            <xsd:element name="Nro_de_resolucion" type="xsd:string"/>
            <xsd:element name="Fecha_de_resolucion" type="xsd:date"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="STTipo_resolucion_informada">
        <xsd:annotation>
            <xsd:documentation>Tipo de resolución informada</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Aplica recargo</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Modifica recargo</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation>Pone término al recargo</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation>Deja sin efecto el recargo</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="CTRecargo_tasa">
        <xsd:annotation>
            <xsd:documentation>Información del recargo de tasa</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Causal_de_recargo" type="STCausal_recargo"/>
            <xsd:element name="Proceso_asociado_al_recargo" type="STTipo_proceso"/>
            <xsd:element name="Otro_proceso_asociado_al_recargo" type="xsd:string" minOccurs="0"/>
            <xsd:element name="Nro_Trabajadores_entidad_empleadora_TT" type="xsd:integer"/>
            <xsd:element name="Magnitud_de_incumplimiento_TA" type="xsd:integer"/>
            <xsd:element name="Porcentaje_base_recargo" type="STPorcentaje_base_recargo"/>
            <xsd:element name="Porcentaje_del_recargo" type="xsd:decimal"/>
            <xsd:element name="Tasa_adicional_110" type="xsd:decimal"/>
            <xsd:element name="Recargo_resultante" type="xsd:decimal"/>
            <xsd:element name="Tasa_cot_adicional" type="xsd:decimal"/>
            <xsd:element name="Tasa_adicional_con_recargo" type="xsd:decimal"/>
            <xsd:element name="Vigencia" type="xsd:date"/>
            <xsd:element name="Declaracion_CT" type="CTDeclaracion_CT"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="STCausal_recargo">
        <xsd:annotation>
            <xsd:documentation>Tipo de causal que origino el recargo</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Causal letra a) artículo 15 D.S. 67</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Causal letra b) artículo 15 D.S. 67</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation>Causal letra c) artículo 15 D.S. 67</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation>Causal letra d) artículo 15 D.S. 67</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5">
                <xsd:annotation>
                    <xsd:documentation>Causal letra e) artículo 15 D.S. 67</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="6">
                <xsd:annotation>
                    <xsd:documentation>Causal del inciso final del articulo 66 Ley N° 16.744</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="STTipo_proceso">
        <xsd:annotation>
            <xsd:documentation>Tipo de proceso que origino el recargo</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>Prescripción de medidas por accidente grave o fatal</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>Prescripción de medidas por enfermedad profesional</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation>Prescripción de medidas por accidente del trabajo</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation>Prescripción de medidas por proceso de vigilancia</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5">
                <xsd:annotation>
                    <xsd:documentation>Prescripción de medidas por asesoria IPER</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="6">
                <xsd:annotation>
                    <xsd:documentation>Prescripción de medidas por autoevaluación de riesgos críticos</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="7">
                <xsd:annotation>
                    <xsd:documentation>Otro</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="STPorcentaje_base_recargo">
        <xsd:annotation>
            <xsd:documentation>Porcentaje base del recargo</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation>20%</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation>25%</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation>32%</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation>40%</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5">
                <xsd:annotation>
                    <xsd:documentation>50%</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="CTDeclaracion_CT">
        <xsd:annotation>
            <xsd:documentation>Declaracion de Centros de trabajo involucrados</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Centro_de_Trabajo" type="CTCentro_trabajo" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CTCentro_trabajo">
        <xsd:annotation>
            <xsd:documentation>Declaracion de Centros de trabajo involucrados</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="CUV" type="STCUV"/>
            <xsd:element name="Rut_empleador_o_Rut_trabajador_independiente" type="STRut"/>
            <xsd:element name="Rut_empleador_principal" type="STRut"/>
            <xsd:element name="Geo_latitud" type="STCoordenadaLatitud"/>
            <xsd:element name="Geo_longitud" type="STCoordenadaLongitud"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
